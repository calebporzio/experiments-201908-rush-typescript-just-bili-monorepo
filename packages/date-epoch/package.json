{
  "name": "@frontend-bindings/date-epoch",
  "version": "1.1.0",
  "description": "Date internal representation utilities using Numbers of seconds since January 1st 1970 (a.k.a. Epoch)",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "main": "dist",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rush-run-all build:*",
    "build:browser": "rush-bili --target browser --format cjs --file-name browser.cjs.js --quiet",
    "build:node": "rush-bili --target node --format cjs --file-name index.js --quiet",
    "build:node-esm": "rush-bili --target node --format esm --file-name index.esm.js --quiet",
    "clean": "just-scripts clean",
    "fix": "just-scripts fix",
    "lint": "just-scripts lint",
    "lint:fix": "just-scripts lint --fix",
    "test": "just-scripts test",
    "test:snapshots": "just-scripts test --snapshots"
  },
  "prettier": "@frontend-bindings/conventions-config-prettier",
  "dependencies": {
    "@babel/preset-env": "^7.4.0",
    "@babel/runtime-corejs3": "~7.5.5"
  },
  "devDependencies": {
    "@frontend-bindings/conventions-config-bili": "~1.0.0",
    "@frontend-bindings/conventions-config-eslint": "~1.0.0",
    "@frontend-bindings/conventions-config-jest": "~1.0.0",
    "@frontend-bindings/conventions-config-prettier": "~1.0.0",
    "@frontend-bindings/conventions-config-typescript-3": "~1.0.0",
    "@frontend-bindings/tools-bundling-helpers": "~1.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.14.17",
    "core-js": "^3.1.4",
    "just-scripts": "^0.28.1"
  }
}
